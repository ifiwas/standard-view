{
  "name": "@standard/view",
  "version": "2.0.0",
  "description": "3D Graphics Library for React Apps",
  "repository": "standard-ai/standard-view",
  "license": "MIT",
  "contributors": [
    "Standard Cognition (https://standard.ai/) <jobs@standard.ai>",
    "Wasif S. Islam",
    "Maytee Chinavanichkit",
    "Francisco Presencia (https://francisco.io/)"
  ],
  "main": "./index.js",
  "scripts": {
    "build": "vite build && yarn build-babel && yarn build-tsc",
    "build-dev": "vite build --mode development",
    "dev": "vite",
    "build-babel": "babel --extensions '.ts,.tsx,.js,.jsx' src/ -d lib/ --copy-files",
    "build-storybook": "storybook build",
    "build-tsc": "tsc",
    "build-flow": "flow-copy-source src lib",
    "flow": "flow",
    "lint": "eslint --cache --format codeframe --ext ts,tsx,mjs,jsx,js src stories",
    "lint-fix": "eslint --cache --format codeframe --ext mjs,jsx,js src stories --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\" \"stories/**/*.{ts,tsx,js,jsx,json}\"",
    "format-check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\" \"stories/**/*.{ts,tsx,js,jsx,json}\"",
    "test": "jest",
    "prepublish": "yarn build",
    "storybook": "storybook dev -p 5001"
  },
  "keywords": [
    "standard-view",
    "standard-view-core",
    "standard",
    "view",
    "core",
    "@react-three/fiber",
    "three",
    "three.js",
    "visualization",
    "tool",
    "package",
    "3D",
    "graphics"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@react-three/drei": "^10.5.1",
    "@react-three/fiber": "^9.2.0",
    "@storybook/addon-docs": "9.0.16",
    "@storybook/addon-links": "9.0.16",
    "@storybook/cli": "^9.0.16",
    "@storybook/react-vite": "9.0.16",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.178.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-react": "^4.0.0",
    "assert": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^10.0.0",
    "eslint": "^9.31.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "9.0.16",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "mousetrap": "^1.6.3",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^9.0.16",
    "three": "^0.178.0",
    "ts-jest": "^29.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5",
    "vite": "^7.0.4",
    "webpack": "^5.100.1"
  },
  "peerDependencies": {
    "react": "^19",
    "three": "^0.178.0"
  },
  "dependencies": {}
}
